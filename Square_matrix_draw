x = 6
y = "+"
h = "-"
f = "|"
n = "*"
temp = x

print(y, end=" ")
for z in range(1,x-1):
    print(h,end=" ")
print(y, end=" ")
print()

for c in range (1,x-1):
    print(f,end=" ")
    for b in range(1,x-1):
        print(h,end = " ")
    print(f,end=" ")
    print()

print(y, end=" ")
for z in range(1,x-1):
    print(h,end=" ")
print(y, end=" ")
print()
